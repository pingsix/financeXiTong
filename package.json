{
  "name": "angular-lazyload-webpack",
  "version": "1.0.0",
  "description": "Lazy load AngularJS modules using Webpack and AngularUI Router",
  "scripts": {
    "start": "webpack --config ./config/webpack.config.js NODE_ENV=development --progress --watch ",
    "build": "webpack --config ./config/webpack.config.js NODE_ENV=dist --progress",
    "devs": "webpack-dev-server --config ./config/webpack.server.js --hot",
    "zip": "node ./tools/pack-zip.js",
    "svn": "node ./tools/upsvn.js",
    "stop": "rimraf build"
  },
  "author": "BR",
  "license": "MIT",
  "dependencies": {
    "@bairong/br_angular_file_upload": "^1.0.0",
    "echarts": "^3.3.1",
    "http-server": "^0.8.0",
    "jquery": "^1.8.2",
    "md5": "^2.2.1",
    "transfer-webpack-plugin": "^0.1.4"
  },
  "devDependencies": {
    "@bairong/dialog_mask": "^1.0.0",
    "@bairong/pro_jstree": "^1.0.6",
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.8",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "html-webpack-plugin": "^2.24.1",
    "html-withimg-loader": "^0.1.16",
    "http-server": "^0.8.5",
    "json-loader": "^0.5.4",
    "jstree": "^3.3.3",
    "live-reload": "^1.1.0",
    "node-sass": "^3.11.2",
    "oclazyload": "^1.0.9",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
